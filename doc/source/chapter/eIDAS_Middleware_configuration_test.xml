<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<EidasMiddlewareConfig xmlns="http://www.governikus.de/2021/03/eumw-config">
    <EidConfiguration>
        <DvcaConfiguration ServerSSLCertificateName="dvcaServerCertificate"
                           BlackListTrustAnchorCertificateName="blackListTrustAnchor"
                           MasterListTrustAnchorCertificateName="masterListTrustAnchor">
            <Name>Governikus test</Name>
            <TerminalAuthServiceUrl>https://dvca-r1.governikus-eid.de/gov_dvca/ta-service</TerminalAuthServiceUrl>
            <RestrictedIdServiceUrl>https://dvca-r1.governikus-eid.de/gov_dvca/ri-service</RestrictedIdServiceUrl>
            <PassiveAuthServiceUrl>https://dvca-r1.governikus-eid.de/gov_dvca/pa-service</PassiveAuthServiceUrl>
            <DvcaCertificateDescriptionServiceUrl>https://dvca-r1.governikus-eid.de/gov_dvca/certDesc-service
            </DvcaCertificateDescriptionServiceUrl>
        </DvcaConfiguration>
        <TimerConfiguration>
            <CertRenewal Length="2" Unit="Hours">
                <HoursRefreshCVCBeforeExpires>48</HoursRefreshCVCBeforeExpires>
            </CertRenewal>
            <BlacklistRenewal Length="2" Unit="Hours"/>
            <MasterAndDefectListRenewal Length="2" Unit="Hours"/>
            <CrlRenewal Length="12" Unit="Hours"/>
        </TimerConfiguration>
        <AllowedEidMeans>A,ID,UB</AllowedEidMeans>
    </EidConfiguration>
    <KeyData>
        <Certificate>
            <Name>blackListTrustAnchor</Name>
            <Certificate></Certificate>
        </Certificate>
        <Certificate>
            <Name>masterListTrustAnchor</Name>
            <Certificate></Certificate>
        </Certificate>
        <Certificate>
            <Name>dvcaServerCertificate</Name>
            <Certificate></Certificate>
        </Certificate>
    </KeyData>
</EidasMiddlewareConfig>
